{
  "name": "@broccoliapps/infra",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist/",
    "create-ssl": "f() { cdk deploy --all --require-approval never --profile appi --app \"tsx src/ssl.ts $1 $2\"; }; f"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.967.0",
    "@aws-sdk/credential-providers": "^3.967.0",
    "aws-cdk-lib": "^2.233.0",
    "constructs": "^10.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}
