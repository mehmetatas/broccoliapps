{
  "name": "@apppotato/expense-tracker-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "cd android && rm -rf build app/build && cd - && react-native run-android",
    "android:clean": "cd android && rm -rf build app/build .gradle && ./gradlew clean && cd - && react-native run-android",
    "ios": "bash -c 'react-native run-ios --mode Debug --simulator \"${1:-iPhone 16}\"' _",
    "ios:clean": "cd ios && rm -rf Pods Podfile.lock build .xcuserdata && pod install && cd - && npm run ios",
    "ios:install:debug": "cd ios && rm -rf Pods Podfile.lock build .xcuserdata && pod install && cd - && react-native run-ios --mode Debug --device \"iPhone 16\"",
    "ios:install:release": "cd ios && rm -rf Pods Podfile.lock build .xcuserdata && pod install && cd - && react-native run-ios --mode Release --device \"iPhone 16\"",
    "ios:publish": "./scripts/ios-publish.sh",
    "ios:testflight": "./scripts/ios-testflight.sh",
    "kill:metro": "pkill -f 'react-native.*start|@react-native/community-cli-plugin.*start|metro' || true",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^8.6.0",
    "@react-navigation/bottom-tabs": "^7.9.0",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.0",
    "lucide-react-native": "^0.562.0",
    "react": "19.2.0",
    "react-native": "0.83.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-mmkv": "^4.1.0",
    "react-native-nitro-modules": "^0.32.0",
    "react-native-reanimated": "^3.19.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.19.0",
    "react-native-svg": "^15.15.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.0",
    "@react-native/eslint-config": "0.83.0",
    "@react-native/metro-config": "0.83.0",
    "@react-native/typescript-config": "0.83.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.2.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.2.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
