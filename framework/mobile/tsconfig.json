{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "lib": ["es2023"],
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true
  },
  "include": ["src"]
}
